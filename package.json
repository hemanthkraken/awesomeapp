{
    "name": "AwesomeApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "cleanup": "rm -rf ios/build; rm -rf android/app/build/; rm -rf ios/Pods/; rm e2e/cucumber-report-ios.json; rm e2e/cucumber-report-android.json; rm yarn-error.log; rm -rf android/.gradle; rm -rf node_modules; rm cucumber_report.html",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "e2e:ios": "configuration=\"ios\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-ios.json",
        "e2e:android:device": "configuration=\"android:device\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-android.json",
        "e2e:android:emulator:debug": "configuration=\"android:emulator:debug\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-android.json",
        "e2e:android:emulator:release": "configuration=\"android:emulator:release\" node ./node_modules/cucumber/bin/cucumber-js ./e2e/ -f node_modules/cucumber-pretty -f json:e2e/cucumber-report-android.json",
        "report": "node e2e/index.js"
    },
    "dependencies": {
        "@react-native-community/checkbox": "^0.5.1",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/toolbar-android": "^0.1.0-rc.2",
        "@react-navigation/bottom-tabs": "^5.8.0",
        "@react-navigation/drawer": "^5.9.0",
        "@react-navigation/material-bottom-tabs": "^5.2.16",
        "@react-navigation/native": "^5.7.3",
        "@react-navigation/stack": "^5.9.0",
        "react": "^16.13.1",
        "react-devtools": "^4.8.2",
        "react-native": "^0.63.2",
        "react-native-gesture-handler": "^1.8.0",
        "react-native-paper": "^4.1.0",
        "react-native-reanimated": "^1.13.0",
        "react-native-safe-area-context": "^3.1.7",
        "react-native-screens": "^2.10.1",
        "react-native-simple-markdown": "^1.1.0",
        "react-native-vector-icons": "^7.0.0",
        "react-native-windows": "^0.63.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/runtime": "^7.11.2",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.13",
        "@types/react-native": "^0.63.15",
        "@types/react-native-vector-icons": "^6.4.5",
        "@types/react-test-renderer": "16.9.3",
        "@typescript-eslint/eslint-plugin": "^4.1.0",
        "@typescript-eslint/parser": "^4.1.0",
        "babel-jest": "^26.3.0",
        "cucumber": "^6.0.5",
        "cucumber-html-reporter": "^5.2.0",
        "cucumber-pretty": "^6.0.0",
        "detox": "17.4.4",
        "eslint": "^7.8.1",
        "eslint-plugin-detox": "^1.0.0",
        "jest": "^26.4.2",
        "jest-circus": "^26.4.2",
        "metro-react-native-babel-preset": "^0.63.0",
        "prettier": "^2.1.1",
        "react-test-renderer": "16.13.1",
        "typescript": "^4.0.2"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
